{
  "name": "voiceflow-ai",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "tsx scripts/production-startup.ts && next start",
    "start:prod": "NODE_ENV=production pnpm start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "migrate:users": "tsx scripts/migrate-existing-users.ts",
    "migrate:users:dry-run": "tsx scripts/migrate-existing-users.ts --dry-run",
    "migrate:users:batch": "tsx scripts/migrate-existing-users.ts --batch-size",
    "verify-setup": "tsx scripts/verify-setup.ts",
    "validate:integration": "tsx scripts/validate-integration.ts",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "dependencies": {
    "@arcjet/inspect": "1.0.0-beta.13",
    "@arcjet/next": "1.0.0-beta.13",
    "@deepgram/sdk": "^4.11.2",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "appwrite": "^21.0.0",
    "assemblyai": "^4.16.1",
    "aws-sdk": "^2.1519.0",
    "bcryptjs": "^3.0.2",
    "better-auth": "^1.3.27",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.545.0",
    "next": "^15.5.4",
    "node-appwrite": "^20.0.0",
    "openai": "^6.2.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "redis": "^4.6.12",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@next/eslint-plugin-next": "^15.5.4",
    "@prisma/client": "6.17.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.37.0",
    "eslint-config-next": "^15.5.4",
    "jsdom": "^27.0.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prisma": "^6.17.0",
    "tailwindcss": "^4.1.7",
    "tsx": "^4.6.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.46.0",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=22.15.0",
    "pnpm": ">=10.17.1"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
